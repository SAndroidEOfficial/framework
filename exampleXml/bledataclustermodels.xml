	<dataClusterModel>
		<!-- left to match with the Cluster in data parser. It doesn't mean anything here -->
		<semantic_id>1</semantic_id>
		<!-- this specifies the data cluster model, shall be modified in the deviceparser implementation -->
		<id>rbs_gio</id>

		<data_handling>
			<type>array</type>
		</data_handling>
		<!-- subdata are listed in List. dataCluster can't have position parameter itself-->
	
		
		<subdata>
			<id>mode</id>
			<!-- position is intended considering the semantic_id -->
			<position>0-0</position>
			<format>uint8</format>
			<bitlogic>
				<operation>and</operation>
				<value>0x0F</value>
			</bitlogic>
			<type>gpio_mode</type>
			<!-- the type are special keywords for the library -->
			<special>
				<value>input:0</value>
				<value>output:1</value>
				<value>analog:2</value>
				<value>pwm:3</value>
				<value>servo:4</value>
			</special>
		</subdata>
		
		<subdata>
			<id>value</id>
			<!-- default format value -->
			<format>uint8</format>
			<!-- default type value -->
			<type>gpio_di</type>
			<position>1-1</position>
			<dependency>
				<id>mode</id>
				<dependencyformat>uint8</dependencyformat>
				<subconfig>
					<type>gpio_di</type>
					<dependencyvalue>0</dependencyvalue>
					<position>1-1</position>
					<format>uint8</format>
				</subconfig>
				
				<subconfig>
					<type>gpio_do</type>
					<dependencyvalue>1</dependencyvalue>
					<position>1-1</position>
					<format>uint8</format>
				</subconfig>
				
				<subconfig>
					<type>gpio_ai</type>
					<dependencyvalue>2</dependencyvalue>
					<position>0-1</position>
					<format>uint16</format>
					<bytelogic>((byte1#RS#4#LS#8)#OR#byte2)</bytelogic>
					<data_handling>
						<type>scaled</type>
					</data_handling>
					<intercept>0</intercept>
					<slope>0.09765625</slope>
				</subconfig>
				
				<subconfig>
					<type>gpio_pwm</type>
					<dependencyvalue>3</dependencyvalue>
					<position>1-1</position>
					<format>uint8</format>
					<data_handling>
						<type>scaled</type>
					</data_handling>
					<intercept>0</intercept>
					<slope>0.39</slope>
				</subconfig>
				
				<subconfig>
					<type>gpio_servo</type>
					<dependencyvalue>4</dependencyvalue>
					<position>1-1</position>
					<format>uint8</format>
					<data_handling>
						<type>scaled</type>
					</data_handling>
					<intercept>0</intercept>
					<slope>0.39</slope>
				</subconfig>
			</dependency>
			
			<!-- position is intended considering the semantic_id -->
		</subdata>
	
	</dataClusterModel>
	
