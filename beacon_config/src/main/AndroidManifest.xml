<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="eu.angel.bleembedded.beaconconfig">


    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:name="eu.angel.bleembedded.lib.activities.SandroideApplication">

        <activity android:name=".MainActivityBeacon"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ClusterActivity"
            android:label="@string/app_name">
        </activity>

        <activity android:name=".MainActivityResearcherNight"
            android:label="@string/app_name">
        </activity>


    </application>

</manifest>
