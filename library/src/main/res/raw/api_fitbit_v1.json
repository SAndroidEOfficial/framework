{
    "auth":{
      "type":"oauth2",  // oauth2 type will launch webview or the browser
      "url":"https://www.fitbit.com/oauth2/authorize?response_type=token&client_id=[oauth2_clientid]&redirect_uri=[redirect_uri]&expires_in=[expires_in]&scope=[scope]"
    },
    "daily_activity_summary": {
      "method":"GET",
      "url":"https://api.fitbit.com/1/user/[user-id]/activities/date/[date].json", //values between parenthesis are variables to be replaced
      "headers":{
        "Authorization":"Bearer [auth.access_token]"
      }
    },
    "activity_time_series": {
      "method":"GET",
      "url":"https://api.fitbit.com/1/user/[user-id]/[resource-path]/date/[base-date]/[end-date].json", //values between parenthesis are variables to be replaced
      "headers":{
        "Authorization":"Bearer [auth.access_token]"
      }
    },
    "activity_intraday_time_series": {
      "method":"GET",
      "url":"https://api.fitbit.com/1/user/-/[resource-path]/date/[date]/[date]/[detail-level]/time/[start-time]/[end-time].json", //values between parenthesis are variables to be replaced
      "headers":{
        "Authorization":"Bearer [auth.access_token]"
      }
    },
